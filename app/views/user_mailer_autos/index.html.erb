<h1>Listing User Mailer Autos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_mailer_autos.each do |user_mailer_auto| %>
      <tr>
        <td><%= link_to 'Show', user_mailer_auto %></td>
        <td><%= link_to 'Edit', edit_user_mailer_auto_path(user_mailer_auto) %></td>
